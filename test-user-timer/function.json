{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "1 */1 * * * *"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "inputBlob",
      "path": "users-db/users.csv",
      "connection": "AzureWebJobsStorage"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outputBlob",
      "path": "users-db/users.csv",
      "connection": "AzureWebJobsStorage"
    }
  ]
}